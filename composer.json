{
    "name": "humbug/box",
    "description": "A tool to simplify building PHARs.",
    "keywords": ["phar"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        },
        {
            "name": "Th√©o Fidry",
            "email": "theo.fidry@gmail.com"
        }
    ],

    "require": {
        "php": "^7.1",
        "herrera-io/annotations": "~1.0",
        "herrera-io/box": "~1.6",
        "herrera-io/json": "~1.0",
        "kherge/amend": "~3.0",
        "justinrainbow/json-schema": "~1.3",
        "phine/path": "~1.0",
        "phpseclib/phpseclib": "~2.0",
        "symfony/console": "^3.0",
        "symfony/finder": "^3.0",
        "symfony/process": "^3.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.2",
        "mikey179/vfsStream": "1.1.0",
        "phpunit/phpunit": "^6.0",
        "symfony/filesystem": "^3.3"
    },
    "suggest": {
        "ext-openssl": "To accelerate private key generation."
    },

    "autoload": {
        "psr-4": {
            "KevinGH\\Box\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KevinGH\\Box\\": ["fixtures", "tests"]
        },
        "files": [
            "src/Test/functions.php"
        ]
    },
    "bin": [
        "bin/box"
    ],

    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "bamarni-bin": {
            "bin-links": false
        }
    }
}
